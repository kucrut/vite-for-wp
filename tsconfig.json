{
	"include": [ "src/**/*.d.ts", "src/**/*.ts", "src/**/*.js" ],
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": "./",
		"checkJs": true,
		"declaration": true,
		"esModuleInterop": true,
		"module": "ES2022",
		"moduleResolution": "Node",
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2020"
	}
}
